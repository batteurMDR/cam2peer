{
  "name": "cam2peer",
  "version": "0.0.1",
  "description": "A videoconference app using P2P",
  "main": "index.html",
  "window": {
    "title": "C2P",
    "toolbar": true,
    "frame": true,
    "resizable": true,
    "position": "center",
    "width": 1250,
    "height": 775
  },
  "repository": {
    "url": "https://github.com/batteurMDR/cam2peer"
  },
  "scripts": {
    "run": "nwbuild --run ./",
    "win": "nwbuild -p win ./",
    "linux": "nwbuild -p linux32,linux64 ./",
    "osx": "nwbuild -p osx ./"
  },
  "author": {
    "name": "Louis Benoist--Foucher",
    "url": "contact@louis-webdesign.com"
  },
  "dependencies": {
    "materialize-css": "^0.96.1",
    "node-webkit-builder": "*"
  },
  "devDependencies": {
    "gulp": "^3.6.2",
    "gulp-shell": "^0.2.5"
  }
}
